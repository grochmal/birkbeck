BITS 64
global _start      ; -- this also prints grochmal.org
fun: pop  rsi      ; import Data.Char
  mov  rax, 0x01   ; r    = [103,11,-3,-12,5,5,-12,11,-62,65,3,-11]
  mov  rdi, 0x01   ; fl s = foldl (\a x -> (head a + x):a) [head s] (tail s)
  mov  rdx, 0x0d   ; fr s = reverse $ map (\x -> chr (fromIntegral x)) $ fl s
  syscall          ; main = do
  mov  rax, 0x3c   ;     putStrLn $ fr r
  mov  rdi, 0x00
  syscall
_start: call fun
  dq 0x6c616d68636f7267  ; lamhcorg
  dq 0x0000000a67726f2e  ; 0000\ngro.
