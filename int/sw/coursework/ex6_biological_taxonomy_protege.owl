###  http://www.semanticweb.org/mgroch01/ontologies/2014/0/ex6_biological_taxonomy#Animal

:Animal rdf:type owl:Class ;
        
        owl:disjointWith :Plant .



###  http://www.semanticweb.org/mgroch01/ontologies/2014/0/ex6_biological_taxonomy#Carnivore

:Carnivore rdf:type owl:Class ;
           
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty :eats ;
                                                        owl:someValuesFrom :Animal
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :eats ;
                                                        owl:allValuesFrom :Animal
                                                      ]
                                                    )
                               ] .



###  http://www.semanticweb.org/mgroch01/ontologies/2014/0/ex6_biological_taxonomy#CarnivorousPlant

:CarnivorousPlant rdf:type owl:Class ;
                  
                  owl:equivalentClass [ rdf:type owl:Class ;
                                        owl:intersectionOf ( :Carnivore
                                                             :Plant
                                                           )
                                      ] .



###  http://www.semanticweb.org/mgroch01/ontologies/2014/0/ex6_biological_taxonomy#Herbivore

:Herbivore rdf:type owl:Class ;
           
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty :eats ;
                                                        owl:someValuesFrom :Plant
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :eats ;
                                                        owl:allValuesFrom :Plant
                                                      ]
                                                    )
                               ] .



